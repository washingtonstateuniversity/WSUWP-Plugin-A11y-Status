{
	"name": "@wsuwp/wsuwp-plugin-a11y-status",
	"version": "1.0.0",
	"description": "A plugin to monitor the WSU Accessibility Training certification status of registered WP users.",
	"author": "Adam Turner",
	"contributors": [
		{
			"name": "washingtonstateuniversity"
		}
	],
	"license": "GPL-2.0-or-later",
	"keywords": [
		"WordPress",
		"plugin",
		"accessibility",
		"users"
	],
	"homepage": "https://github.com/washingtonstateuniversity/wsuwp-plugin-a11y-status/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/washingtonstateuniversity/wsuwp-plugin-a11y-status"
	},
	"bugs": {
		"url": "https://github.com/washingtonstateuniversity/wsuwp-plugin-a11y-status/issues"
	},
	"browserslist": [
		"> 1%",
		"ie 8-11",
		"Firefox ESR"
	],
	"devDependencies": {
		"cssnano": "^4.1.x",
		"npm-run-all": "^4.1.x",
		"postcss-cli": "^6.1.3",
		"postcss-preset-env": "^6.7.0",
		"rimraf": "^2.6.x",
		"stylelint": "^10.1.0"
	},
	"scripts": {
		"build": "run-s clean test 'build:*'",
		"build:styles": "postcss 'src/_css/*.css' --dir css/",
		"clean": "rimraf css/",
		"lint:styles": "stylelint 'src/**/*.css'",
		"lint:pkg": "npmPkgJsonLint .",
		"lint:php": "composer lint-detail",
		"start": "run-p 'build:*'",
		"test": "run-p 'lint:*'"
	}
}
